{
  "root": "api",
  "targets": {
    "test": {},
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "api",
        "command": "go build -o dist/api cmd/main/main.go"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "api",
        "command": "go run cmd/main/main.go"
      }
    }
  }
}